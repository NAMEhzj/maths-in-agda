open import Group
open import Sets
open import Equivalence hiding (_∘_)
open import Agda.Primitive
open import PropTruncation


module GroupProps {l} (G : Set l) (G' : Group G) where  

open Group.Group G'

record Subgroup {b} (U : 𝒫 G {b}) : Set (l ⊔ b) where
                field
                  e∈U : e ∈ U
                  ∘-closed : (x y : G) → x ∈ U → y ∈ U → x ∘ y ∈ U 
                  inv-closed : (x : G) → x ∈ U → x ⁻¹ ∈ U


